{
  "id": "dependabot-config-minimal",
  "name": "Dependabot 設定 最小",
  "version": "1.0.0",
  "stack": ["github"],
  "tags": ["deps"],
  "description": "Dependabot の最小設定。",
  "params": [ { "name": "app_name", "default": "dependabot" } ],
  "files": [
    { "path": "{{app_name}}/.github/dependabot.yml", "template": "version: 2\nupdates:\n  - package-ecosystem: npm\n    directory: /\n    schedule:\n      interval: weekly\n" }
  ],
  "patches": []
}

