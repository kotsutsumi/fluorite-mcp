{
  "id": "nextjs-auth0-minimal",
  "name": "Next.js Auth0 Minimal",
  "version": "1.0.0",
  "stack": ["nextjs", "auth0"],
  "tags": ["auth"],
  "description": "Auth0 SDK minimal setup with route handler.",
  "params": [{ "name": "app_name", "default": "next-app" }],
  "files": [
    { "path": "{{app_name}}/app/api/auth/[auth0]/route.ts", "template": "export { GET, POST } from '@auth0/nextjs-auth0/edge';\n" },
    { "path": "{{app_name}}/middleware.ts", "template": "export { auth as middleware } from '@auth0/nextjs-auth0/edge';\n" },
    { "path": "{{app_name}}/.env.example", "template": "AUTH0_SECRET=\nAUTH0_BASE_URL=http://localhost:3000\nAUTH0_ISSUER_BASE_URL=\nAUTH0_CLIENT_ID=\nAUTH0_CLIENT_SECRET=\n" }
  ],
  "patches": []
}

